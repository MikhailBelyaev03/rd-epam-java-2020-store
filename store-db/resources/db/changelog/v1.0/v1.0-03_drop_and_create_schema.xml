<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog">
    <changeSet id="drop-schema" author="Belousov">
        <!-- Drop schema -->
        <sql dbms="postrgesql" endDelimiter=";">
            drop schema if exists store cascade
        </sql>

        <!-- Create schema -->
        <sql dbms="postresql" endDelimiter=";">
            create schema store
        </sql>
    </changeSet>
</databaseChangeLog>

